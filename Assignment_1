class Student {
    int rollNo;
    String name;
    double percentage;


    Student(int r, String n, double p) {
        rollNo = r;
        name = n;
        percentage = p;
    }


    void displayStudent() {
        System.out.println("Roll No: " + rollNo);
        System.out.println("Name: " + name);
        System.out.println("Percentage: " + percentage);

    }
}

public class Main {
    public static void main(String[] args) {

        Student s1 = new Student(101, "Aarav", 88.5);
        Student s2 = new Student(102, "Neha", 91.2);

        s1.displayStudent();
        s2.displayStudent();
    }
}
